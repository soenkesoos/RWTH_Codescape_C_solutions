#include "codescape.h
void run() {
    
	char moves[] = "mmmllmllmmmmrmpllmrm";
	
    for (int i = 0; moves[i] != '\0'; i++) {
        
        switch (moves[i]) {
            case 'm':
                move();
                break;
            case 'l':
                turnLeft();
                break;
            case 'r':
                turnRight();
                break;
            case 'p':
                pickUp();
        }
    }
}
